	i2c@78ba000 { /* BLSP1 QUP6 */
		status = "okay";

		nfc-nci@e {
			status = "disabled";
		};
	};

	i2c@78b6000 { /* BLSP1 QUP2 */
		/delete-node/ mpu6050@68;
		/delete-node/ avago@39;
		/delete-node/ akm@c;

		TMD2771@39 {
			compatible = "TMD2771";
			vdd-supply = <&pm8916_l17>;
			vddio-supply = <&pm8916_l6>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <113 0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&apds99xx_default>;
			reg = <0x39>;
			irq-gpio = <&msm_gpio 113 0x00>;
		};

		lis3dh_acc@18 {
			compatible = "lis3dh_acc";
			reg = <0x18>;
			vdd-supply = <&pm8916_l17>;
			vddio-supply = <&pm8916_l6>;

			interrupt-parent = <&msm_gpio>;
			interrupts = <21 0x00>;
			st,min-interval = <10>;
			st,init-interval = <200>;
			st,axis-map-x = <1>;
			st,axis-map-y = <0>;
			st,axis-map-z = <2>;
			st,g-range = <2>;
			st,negate-x = <0>;
			st,negate-y = <1>;
			st,negate-z = <0>;
			sensor,gpio-irq = <&msm_gpio 21 0x00>;
		};
		ALPS007@0C {
    		compatible = "alps,hscdtd007a";
    		reg = <0x0C>;
    		alps,layout =<5>;
    		alps,auto-report=<1>;
    		vdd-supply = <&pm8916_l17>;
    		vio-supply = <&pm8916_l6>;
    		};
		yas533@2E {
			compatible = "yas533_mag";
			reg = <0x2E>;
			ak,layout =<6>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l6>;

		};
		akm8963@0D {
			compatible = "akm,akm8963";
			reg = <0x0D>;
			ak,layout =<4>;
			ak,auto-report=<1>;
			vdd-supply = <&pm8916_l17>;
			vio-supply = <&pm8916_l6>;
			iic-address=<0x0C>;
		};
		akm09912@0F{
		    compatible = "ak,ak09912";
		    reg = <0x0F>;
		    vdd-supply = <&pm8916_l17>;
		    vio-supply = <&pm8916_l6>;
		    akm,layout = <8>;
		    akm,auto-report=<1>;
		    iic-address=<0x0C>;
	    };

	};
	
	
		/* VIVO_NFC */
	i2c@78ba000 { /* BLSP1 QUP6 */
		status = "okay";
		nfc_pn5xx@28 {
			compatible = "nxp,pn547";
			reg = <0x28>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <108 0x2008>;
			pinctrl-names = "default";
			pinctrl-0 = <&pn5xx_gpio_active_0 &pn5xx_gpio_active_1>;
			nxp,pn544-ven = <&msm_gpio 0 0x00>;
			nxp,pn544-fw-dwnld = <&msm_gpio 3 0x00>;
			nxp,pn544-irq = <&msm_gpio 108 0x00>;
			nxp,pn544-ese-pwr = <&msm_gpio 2 0x00>;
			nxp,pn5xx-clkreq = <&pm8916_gpios 2 0x00>;
			nxp,pn5xx-pvdden_gpio = <&msm_gpio 118 0x00>;
			clock-names = "ref_clk";
			clocks = <&clock_rpm clk_bb_clk2_pin>;
		};
	};
	

